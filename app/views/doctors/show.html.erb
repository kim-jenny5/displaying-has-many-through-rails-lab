<h1>Your Appointments</h1>
<table>
    <tr>
        <th>Patient Name</th>
        <th>Appointment Time</th>
    </tr>
    <% @doctor.appointments.each do |appt| %>
    <tr>
        <td><%= link_to appt.patient.name, patient_path(appt.patient) %></td>
        <td><%= link_to appt.appointment_datetime.strftime("%B %-d, %Y at %H:%M"), appt %></td>
    </tr>
    <% end %>
</table>